<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="oss.mapper.BusiManageMapper">
	<update id="DlUserID" parameterType="long">
	update  Users set userPwd =123456 where userID=#{dl}
	</update>
	
	<update id="Blacklist" parameterType="long">
	update  Users set userStatusID =3 where userID=#{dl}
	</update>
	
	<update id="Disable" parameterType="long">
	update  Users set userStatusID =2 where userID=#{dl}
	</update>
	
	<select id="listUser" resultType="oss.bean.Users">
		select * from Users where 1 = 1
	</select>
	
	<update id="providerupdatepow" parameterType="long">
	update  Users set userPwd =123456 where userID=#{dl}
	</update>
	
	<update id="providerBlacklist" parameterType="long">
	update  Users set userStatusID =3 where userID=#{dl}
	</update>
	
	<update id="providerDisable" parameterType="long">
	update  Users set userStatusID =2 where userID=#{dl}
	</update>
	
	<select id="providerlist" resultType="oss.bean.Users">
		select * from Users where 1 = 1
	</select>
	
</mapper>
