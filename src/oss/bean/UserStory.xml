<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="oss.mapper.PortalManageMapper">
	<select id="storyList" resultType="oss.bean.UserStory">
		select * from UserStory where 1 = 1
	</select>
	
	<update id="updateStory" parameterType="oss.bean.UserStory">
	    update UserStory set storyTitle=#{storyTitle},storyContext=#{storyContext},storyTime=#{storyTime}
	    where storyID=#{storyID}
	</update>
</mapper>
